  "name": "visual-platform",
  "version": "2.6.0",
  "private": true,
  "scripts": {
    "a11y": "pa11y http://localhost:3000",
    "backup": "bash scripts/auto-backup.sh",
    "build": "next build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:studio": "drizzle-kit studio",
    "deploy": "bash scripts/deploy-with-rollback.sh deploy",
    "dev": "next dev",
    "docker:build": "docker-compose build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:up": "docker-compose up -d",
    "docs:generate": "typedoc --out docs/api src",
    "docs:serve": "http-server docs -p 8080",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "health": "curl -f http://localhost:5000/healthz || exit 1",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "rollback": "bash scripts/quick-rollback.sh",
    "security:audit": "yarn audit --level moderate",
    "security:fix": "yarn audit --fix",
    "start": "next start",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run tests/e2e",
    "test:unit": "vitest run --coverage",
    "type-check": "tsc --noEmit"
